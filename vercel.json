{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/announcements",
        "dest": "/announcements"
      },
      {
        "src": "/maintenance",
        "dest": "/maintenance"
      },
      {
        "src": "/committee",
        "dest": "/committee"
      },
      {
        "src": "/financial-reports",
        "dest": "/financial-reports"
      },
      {
        "src": "/contact-feedback",
        "dest": "/contact-feedback"
      },
      {
        "src": "/",
        "dest": "/"
      }
    ],
    "env": {
      "NEXT_PUBLIC_SITE_NAME": "Strata Management Portal",
      "NEXT_PUBLIC_CONTACT_EMAIL": "contact@stratamanagement.com"
    },
    "alias": "strata-management-portal"
  }
  